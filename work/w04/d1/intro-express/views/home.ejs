<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Home Page</h1>
    <h4>Hello <%= name %></h4>
    <h5><% 4 + 4 %></h5>
    <ul>
        <% todos.forEach(function(t) { %>
          <li>
            <%= t.todo %>
            <strong>------</strong>
            <%= t.done ? 'done' : 'not done' %>
          </li>
        <% }); %>
      </ul>
</body>
</html>